import { toast } from 'react-toastify';

export const InfoMsg = (message:any, type: keyof typeof toast) => {
    (toast as any)[type](message, {
      position: "top-right",
      autoClose: 5000,
      hideProgressBar: false,
      closeOnClick: true,
      pauseOnHover: true,
      draggable: true,
      progress: undefined,
      theme: "light",
    });
    return {status: 201}
  };